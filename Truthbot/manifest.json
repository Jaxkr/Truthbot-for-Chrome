{
  "manifest_version": 2,

  "name": "Truthbot",
  "description": "View information about news sites and articles via Truthbot.org.",
  "version": "0.2",
  "icons": { "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
